import java.util.*;

public class Main{

	public static void main(String[] args){
		Queue<Integer> q= new LinkedList<>();
		
		q.offer(5);
		q.offer(2);
		q.offer(3);
		q.offer(7);
		q.poll();
		q.offer(1);
		q.offer(4);
		q.poll();
		
		먼저 들어온 원소부터 추출한다.
		while(!q.isEmpty()){
			System.out.print(q.poll()+" ");
			}
		}
	}
	
	실행결과
	3 7 1 4